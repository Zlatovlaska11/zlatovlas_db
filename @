mod pager {

    const PAGE_SIZE: usize = 4096; // 4 KB page size
    static ID: usize = 0;

    struct Page {
        id: usize,
        data: [u8; PAGE_SIZE],

        modified: bool,
    }


    impl Page {

        pub fn new() -> Self {
            
            Page {
                id: ID,
                modified: false,
                data: 


            }

        }

    }
}
